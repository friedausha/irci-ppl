<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="IRCI.MenuController">
   <children>
      <VBox prefHeight="64.0" prefWidth="600.0">
         <children>
            <HBox prefHeight="64.0" prefWidth="262.0">
               <children>
                  <VBox prefHeight="200.0" prefWidth="100.0">
                     <children>
                        <Button fx:id="InsertMetadataButton" mnemonicParsing="false" onAction="#displayForm" prefHeight="64.0" prefWidth="104.0" text="Insert Metadata" />
                     </children>
                  </VBox>
                  <VBox prefHeight="200.0" prefWidth="100.0">
                     <children>
                        <Button fx:id="ViewGraphButton" mnemonicParsing="false" onAction="#readRan" prefHeight="64.0" prefWidth="87.0" text="View Graph" />
                     </children>
                     <padding>
                        <Insets left="10.0" />
                     </padding>
                  </VBox>
                  <VBox prefHeight="200.0" prefWidth="234.0">
                     <children>
                        <TextField fx:id="SearchTextField" />
                        <Button fx:id="SearchbyNameButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="232.0" text="Search by Researcher's Name" />
                        <Button fx:id="SearchbyTopicButton" mnemonicParsing="false"  prefHeight="25.0" prefWidth="247.0" text="Search by Topic" />
                     </children>
                  </VBox>
               </children>
            </HBox>
         </children>
      </VBox>
      <Label layoutX="-89.0" layoutY="38.0" text="Label" />
   </children>
</AnchorPane>
